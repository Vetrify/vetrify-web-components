{
  "name": "vetrify-web-components",
  "version": "0.0.1",
  "description": "Vetrify Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/vetrify-web-components/vetrify-web-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "themes/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "dev": "npm run make-icons && stencil build --dev --docs --watch --serve",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "make-dist": "node make-dist.js",
    "make-icons": "node make-icons.js",
    "prettier": "prettier --write --loglevel warn .",
    "postbuild": "npm run make-dist",
    "prebuild": "npm run prettier && npm run lint && npm run make-icons",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run storybook\"",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "build-storybook": "build-storybook -s ./dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-typescript": "^7.13.0",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-controls": "^6.1.19",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/html": "^6.1.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/web-components": "^6.1.20",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-loader": "^8.2.2",
    "bluebird": "^3.7.2",
    "bootstrap-icons": "^1.3.0",
    "browser-sync": "^2.26.13",
    "chalk": "^4.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^5.1.0",
    "del": "^5.1.0",
    "download": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.2",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^1.0.4",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lit-html": "^1.3.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.5.0",
    "recursive-copy": "^2.0.10",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "through2": "^3.0.1",
    "typescript": "^4.0.2",
    "workbox-build": "4.3.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "@shoelace-style/animations": "^1.0.0",
    "@stencil/core": "^2.4.0",
    "@types/resize-observer-browser": "^0.1.4",
    "color": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint"
    }
  }
}
